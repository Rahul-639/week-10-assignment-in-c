#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#define MAX_LEN 1000000
#define MAX_OPS 100000
typedef struct {
    int type;       
    char *str;      
} Operation;

char S[MAX_LEN + 5];  
int len = 0;          
Operation stack[MAX_OPS];
int top = -1;
void pushOp(int type, const char *str) {
    stack[++top].type = type;
    stack[top].str = strdup(str); 
}
Operation popOp() {
    return stack[top--];
}
int main() {
    int Q;
    scanf("%d", &Q);
    while (Q--) {
        int type;
        scanf("%d", &type);
        if (type == 1) {
            char w[MAX_LEN + 5];
            scanf("%s", w);
            int l = strlen(w);
            strcpy(S + len, w);
            len += l;
            pushOp(1, w);
        } else if (type == 2) {  
            int k;
            scanf("%d", &k);
            char removed[MAX_LEN + 5];
            strncpy(removed, S + len - k, k);
            removed[k] = '\0';
            len -= k;
            S[len] = '\0';
            pushOp(2, removed);
        } else if (type == 3) { 
            int k;
            scanf("%d", &k);
            if (k <= len) {
                printf("%c\n", S[k - 1]);
            }
        } else if (type == 4) {  
            if (top >= 0) {
                Operation last = popOp();
                if (last.type == 1) {
                    int l = strlen(last.str);
                    len -= l;
                    S[len] = '\0';
                } else if (last.type == 2) {
                    int l = strlen(last.str);
                    strcpy(S + len, last.str);
                    len += l;
                }
                free(last.str);
            }
        }
    }
    return 0;
}
